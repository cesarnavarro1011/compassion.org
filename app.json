{
  "expo": {
    "name": "compassion",
    "slug": "compassion",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./asset/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./asset/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./asset/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./asset/favicon.png"
    },
    "plugins": [
      "expo-build-properties"
    ]
  }
}
